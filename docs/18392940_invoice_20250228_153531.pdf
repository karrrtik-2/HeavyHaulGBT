<script>
document.addEventListener("DOMContentLoaded", () => {
    let printLink = document.getElementById("printer");
    let container = document.getElementById("invoice-content");

    printLink.addEventListener("click", event => {
        event.preventDefault();
        printLink.style.display = "none";
        window.print();
    }, false);

    container.addEventListener("click", event => {
        printLink.style.display = "flex";
    }, false);

}, false);
</script>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jspdf/1.3.5/jspdf.min.js"></script>
<script src="https://permits.synchrontms.com/assets/js/custom-invoice-print.js"></script>
<script>
document.getElementById('create_pdf').addEventListener('click', () => {
    // Get the content you want to include in the PDF
    const pdfContent = document.getElementById('invoice-content');

    // Save the current color of text elements
    const originalColors = [];
    const textElements = pdfContent.querySelectorAll('p, span, div, td, th, h2, h3, h4, h5, h6');
    textElements.forEach((element, index) => {
        // Check if the element has the class "fw-medium"
        if (!element.classList.contains('fw-medium')) {
            originalColors[index] = element.style.color; // Save original color
            element.style.color = '#000'; // Set color to black
        } else {
            originalColors[index] = null; // Mark as null to indicate no change
        }
    });

    // Create a new jsPDF instance with A4 dimensions (210mm x 297mm)
    const pdf = new jsPDF('p', 'mm', 'a4', true);

    // Convert the content to a canvas using html2canvas
    html2canvas(pdfContent, {
        scale: 2, // Adjust the scale for better image quality
        logging: false // Disable logging to console for cleaner output
    }).then(canvas => {
        // Calculate the aspect ratio for resizing
        const imgWidth = 210; // A4 width in mm
        const imgHeight = (canvas.height * imgWidth) / canvas.width;

        // Add the canvas to the PDF with A4 dimensions
        const imgData = canvas.toDataURL('image/jpeg', 0.6); // Adjust compression quality (0.0 - 1.0)
        pdf.addImage(imgData, 'JPEG', 0, 0, imgWidth, imgHeight);

        // Restore original colors of text elements
        textElements.forEach((element, index) => {
            if (originalColors[index] !== null) {
                element.style.color = originalColors[index]; // Restore original color
            }
        });

        // Save or open the PDF
        pdf.save('Truck 4156 - Invoice 18392940 - WA - NV - SynchronPermits.pdf');
    });
});
</script>
